{
  "name": "zenbustudio/craft-japanese-date",
  "description": "Converts a date string or date object to Japanese format, with the ability to calculate and display the Modern Japan Era name (Reiwa/令和, Heisei/平成, Showa/昭和, Taisho/大正, Meiji/明治)",
  "version": "1.0.0",
  "type": "craft-plugin",
  "keywords": ["japanese", "japan", "meiji", "taisho", "showa", "heisei", "reiwa"],
  "license": "MIT",
  "authors": [
    {
      "name": "Zenbu Studio",
      "homepage": "https://zenbustudio.com"
    }
  ],
  "support": {
    "email": "support@zenbustudio.com",
    "issues": "https://github.com/zenbustudio/craft.japanese-date/issues?state=open",
    "source": "https://github.com/zenbustudio/craft.japanese-date",
    "docs": "https://github.com/zenbustudio/craft.japanese-date/blob/master/README.md"
  },
  "require": {
    "craftcms/cms": "^3.1.0"
  },
  "autoload": {
    "psr-4": {
      "zenbustudio\\japanesedate\\": "src/"
    }
  },
  "extra": {
    "name": "Japanese Date",
    "handle": "japanese-date",
    "class": "zenbustudio\\japanesedate\\JapaneseDate"
  }
}